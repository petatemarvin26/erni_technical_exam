{
  "name": "react-webpack",
  "version": "1.0.5",
  "description": "This is project created from scratch",
  "private": true,
  "scripts": {
    "test": "echo \"HELLO WORLD\"",
    "clean-build": "rm -rf build -r",
    "start": "webpack serve --env dev --config webpack/common.js",
    "build": "npm run clean-build; webpack build --env prod --config webpack/common.js",
    "build:dev": "npm run clean-build; webpack build --env prod --config webpack/dev.js",
    "build:uat": "npm run clean-build; webpack build --env prod --config webpack/uat.js",
    "build:prod": "npm run clean-build; webpack build --env prod --config webpack/prod.js"
  },
  "keywords": [
    "scratch",
    "react"
  ],
  "author": "Marvin Petate",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@svgr/webpack": "^6.5.1",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "babel-loader": "^9.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "dotenv": "^16.0.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "process": "^0.11.10",
    "style-loader": "^3.3.1",
    "svg-inline-loader": "^0.8.2",
    "ts-loader": "^9.4.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-manifest-plugin": "^5.0.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
